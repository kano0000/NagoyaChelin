<table class = "table table-borderd">
  <tr>
    <th></th>
    <th>ユーザー名</th>
    <th colspan="3"></th>
  </tr>
  <tr>
    <% users.each do |user| %>
    <td>
      <%= image_tag user.get_profile_image(100,100) %>
    </td>
    <td>
      <%= user.name %>
    </td>
    <td>フォロー数: <%= user.followings.count %></td>
    <td>フォロワー数: <%= user.followers.count %></td>
    <td><%= link_to "Show", admin_user_path(user) %></td>
  </tr>
  <% end %>
</table>